{
  "name": "harmony",
  "version": "1.0.0",
  "description": "Group music creation application",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iProgYou/harmony.git"
  },
  "author": "Zack Barbieri, Charles Coombs, Isaac Yoon, William B Sexton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iProgYou/harmony/issues"
  },
  "homepage": "https://github.com/iProgYou/harmony#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-icons": "^3.10.0",
    "socket.io": "^2.4.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "keywords": []
}
